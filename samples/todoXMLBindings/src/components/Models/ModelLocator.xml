<?xml version="1.0" encoding="UTF-8"?>
<component name="ModelLocator" extends="Node" 
	>
	<interface>
    <!-- DIP mechanism for app - this is injected into every view
      to allow for IOC -->
    <!-- reference to the main screen -->
    <field id="appController" type="node"/>
    <field id="appArgs" type="assocarray"/>
    <field id="styles" type="assocarray" value="{}"/>
    <field id="localizations" type="assocarray" value="{}"/>
    <field id="assets" type="assocarray" value="{}"/>
    <field id="deepLinkingData" type="assocarray" value="{}"/>
	</interface>

</component>
